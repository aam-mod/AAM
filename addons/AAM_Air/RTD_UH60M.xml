      <MathExpression>
        <Expression>
          latCyclic = latCyclic - clamp(rollRateRad * 2, -0.15 , 0.15 ),
          lonCyclic = lonCyclic + clamp(pitchRateRad * 3, -0.15 , 0.15 ),
          pedals = pedals + clamp(yawRateRad * 3, -0.15 , 0.15 ),
          A1m = latCyclic * 10.5,
          A1m = clamp( A1m , -12.5 , 8.5 ),
          B1m = linearInterp( -13 , 18 , (lonCyclic  +1) * 0.5  ),
          B1m = clamp( B1m , -13 , 18 ),		  
          Th0m= collective * 10.5 + 8.5,
          A1t = 0.0,
          B1t = 0.0,
          Th0t = linearInterp( 30 , -5,  (pedals  +1) * 0.5 ),
          A1m = degToRad(A1m),
          B1m = degToRad(B1m),
          Th0m = degToRad(Th0m),
          Th0t = degToRad(Th0t),
		  		profondeur = lonCyclic*10,
          mainRotorBrake = rotorBrake          

        </Expression>        
        
      </MathExpression>     

    </CSComponent>
    
